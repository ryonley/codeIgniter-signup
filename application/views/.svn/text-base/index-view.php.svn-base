<script type="text/javascript">
    $(document).ready(function(){
        //////  MOVE EVERY OTHER ELEMENT TO THE RIGHT COLUMN (PRESERVES TABBING IN A LOGICAL ORDER //////
        $('#enrollment .topBlock .formfield:odd').css({'float' : 'right' , 'width' : '330px'});
        $('#enrollment .topBlock .formfield:even').css({'float' : 'left' , 'width' : '330px'});
        /////////////////////////////////////////////////////////////////
        
       $('ul#topnav li a:last').css('borderRight', 'none');
         $('#subnav li a:last').css('borderRight', 'none');
         $('ul#primary-nav li:last').addClass('last');

         $('#main  ul li').each(function(){
              var index = $(this).index();
              var index = index + 1;
              var title = 'bullet-'+index;
             var imageurl = 'assets/images/'+title+'.gif';
              $(this).css('background-image', 'url("' + imageurl + '")');
         });
         
         /// FOR TOGGLING THE PRIMARY MEMBER ID FIELD
         $('#prime_member').hide();
         
         extraToggle($('#is_dependent'), $('#prime_member'));
         
         $('#is_dependent').click(function(){
              extraToggle($('#is_dependent'), $('#prime_member'));
         });
         
  
          function extraToggle(checkbox, extra)
            {
                    // If checked
                    if(checkbox.is(":checked"))
                    {
                    //show the hidden div
                    extra.show("fast");
                    }
                    else
                    {
                    //otherwise, hide it
                    extra.hide("fast");
                    }
            }
        ///////////////////////////////////////////////////////////////
        
         
       $('#EffectiveDate').datepicker({
           minDate: 0,
           changeMonth: true,
           changeYear: true
      });
      
      $('#DOB').datepicker({
         yearRange: '-100y:c+nn',
             changeMonth: true,
           changeYear: true
      });
      
      //////////////// FOR VALIDATION //////////////////////////////
      $('#enrollment').validate();
      
      
                //////////////////////// CARDHOLDER IS THE MEMBER SWITCH/////////////////////////////
                
                $('#enrollment #sameAs').click(function() {
                        //IF CHECKED
                        if($('#enrollment #sameAs').is(":checked")) {
                              // FOR EACH 
                              $('#enrollment #billing input', ':visible', document.body).each(function(i) {
                                        $(this).val($('#member div .copy').eq(i).text() );
                              });
                              var memberState = $('#memberState').text();
                              console.log(memberState);
                              $('#State').val(memberState);

                        } else {
                              $('#billing input', ':visible', document.body).each(function(i){
                                    $(this).val("");
                              });
                              $('#State').val("");  
                        }
                });
                
                //////////////// MAKE THE MEMBER DIV ROWS ALTERNATE COLOR
                  $('#member div:even').addClass('highlight');

    });  //END DOCUMENT READY
    
  
    
    
    
</script>



